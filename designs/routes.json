        {
            "component": "signInComponent",
            "path": "signIn"
        },
        {
            "component": "operations_dashboardComponent",
            "path": "operations_dashboard",
            "children": [
                {
                    "path": "view_table_of_receipt",
                    "component": "view_table_of_receiptComponent"
                },
                {
                    "path": "categories",
                    "component": "categoriesComponent"
                },
                {
                    "component": "all_purchased_items_tableComponent",
                    "path": "all_purchased_items_table"
                },
                {
                    "component": "statisticsComponent",
                    "path": "statistics"
                },
                {
                    "path": "landing",
                    "component": "landingComponent"
                },
                {
                    "path": "add_receipt",
                    "component": "add_receiptComponent"
                }
            ]
        },
        {
            "component": "user_dashboardComponent",
            "path": "user_dashboard"
        },
        {
            "component": "selected_receipt_formComponent",
            "path": "selected_receipt_form"
        },
        {
            "path": "view_table_of_receipt",
            "component": "view_table_of_receiptComponent"
        },
        {
            "path": "all_purchased_items_table",
            "component": "all_purchased_items_tableComponent"
        },
        {
            "path": "login_guidelines",
            "component": "login_guidelinesComponent"
        },
        {
            "path": "line_chart",
            "component": "line_chartComponent"
        },
        {
            "path": "donut_chart",
            "component": "donut_chartComponent"
        },
        {
            "path": "''",
            "redirectTo": "/operations_dashboard/landing",
            "pathMatch": "full",
            "default": true
        },
        {
            "path": "**",
            "component": "PageNotFoundComponent"
        }
    ]
}
